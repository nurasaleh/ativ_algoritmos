programa
{
//2. Faça um Programa que peça uma temperatura e informe na tela se o valor é positivo ou negativo.
	
	funcao inicio()
	{
	
	real numero 
		escreva("escreva quantos graus esta fazendo onde voce mora ")
		leia(numero)

		se(numero > 0)
		escreva("positivo")
		senao
		escreva("negativo")
	}
}
