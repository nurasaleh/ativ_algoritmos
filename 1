programa
{
//1. Faça um Programa que peça dois números e imprima o maior deles.
	
	funcao inicio()
	{
	
		inteiro n1, n2

		escreva("escreva um numero " )
		leia(n1)

		escreva("agora escreva outro ")
		leia(n2)

		se(n1 >= n2 ) 
		escreva("o maior numero foi " , n1 )
		senao
		se(n2 >= n1)
		escreva("o maior numero foi " , n2)
